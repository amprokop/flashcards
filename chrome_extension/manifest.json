{
  "name": "Schwa",
  "description": "Dead-simple flashcards.",
  "version": "1.0",
  "manifest_version": 2,

  "background": {
    // "page": "background.html",
    "scripts" : ["background.js"]
  },

  "permissions": [
    "tabs",
    "contextMenus"
    // "http://localhost:8080/"
  ],

  "icons" : {
    "16" : "icon_small.png",
    "128" : "icon_large.png"
  },

  "browser_action" : {
    "default_title" : "Remindr",
    "default_icon": "icon.png"
  }
}